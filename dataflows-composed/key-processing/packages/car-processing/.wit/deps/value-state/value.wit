package sdf:value-state;

// simple primitive resource value
interface values {

  // holds 32 value
  resource value32 {
    get: func() -> s32;
    set: func(val: s32);
    increment: func(val: s32) -> s32;
  }

  resource value64 {
    get: func() -> s64;
    set: func(val: s64);
    increment: func(val: s64) -> s64;
  }
}



world value-world {
   import values;
}